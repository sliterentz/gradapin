{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/api-datasource/(.*)": "<rootDir>/../libs/api-datasource/src/$1",
    "@libs/api-datasource": "<rootDir>/../libs/api-datasource/src"
  }
}