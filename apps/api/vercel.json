{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "installCommand": "pnpm install",
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist"
}